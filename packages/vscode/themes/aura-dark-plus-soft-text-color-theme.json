  "tokenColors": [
    {
      "name": "Accent1",
      "scope": [
        "keyword",
        "storage",
        "support",
        "entity.name.tag",
        "variable.language",
        "keyword.control.flow",
        "storage.modifier",
        "keyword.operator",
        "entity.other.attribute-name.class.css",
        "entity.other.keyframe-offset",
        "markup.heading",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#9479ce"
      }
    },
    {
      "name": "Accent2",
      "scope": [
        "string",
        "markup.inserted",
        "markup.raw",
        "constant"
      ],
      "settings": {
        "foreground": "#68bb9f"
      }
    },
    {
      "name": "Accent3",
      "scope": [
        "markup.changed",
        "entity"
      ],
      "settings": {
        "foreground": "#ccba6d"
      }
    },
    {
      "name": "Accent5",
      "scope": [
        "invalid",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#c76d6d"
      }
    },
    {
      "name": "Accent6",
      "scope": [
        "string.unquoted",
        "punctuation.separator",
        "entity.other.attribute-name",
        "meta.object-literal.key",
        "variable.object.property",
        "variable.other.property",
        "variable.other.object.property",
        "meta.class",
        "meta.type.annotation",
        "meta.method.declaration",
        "support.type.property-name.css",
        "support.type.vendored"
      ],
      "settings": {
        "foreground": "#bb74af"
      }
    },
    {
      "name": "Accent7",
      "scope": [
        "variable",
        "markup.list",
        "support.constant.property-value.css",
        "variable.parameter.keyframe-list.css",
        "source.css",
        "support.constant.font-name",
        "support.constant.vendored.property-value",
        "variable.parameter.tsx"
      ],
      "settings": {
        "foreground": "#c2c2c2"
      }
    },
    {
      "name": "Accent8",
      "scope": "comment",
      "settings": {
        "foreground": "#6d6d6d"
      }
    },
    {
      "name": "Accent31",
      "scope": [
        "entity.name.function",
        "variable.parameter",
        "support.function"
      ],
      "settings": {
        "foreground": "#bcbd81"
      }
    },
    {
      "name": "Accent32",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class.builtin",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.quasi.element.begin",
        "punctuation.quasi.element.end"
      ],
      "settings": {
        "foreground": "#75b5c9"
      }
    },
    {
      "name": "Italics",
      "scope": [
        "meta.parameters",
        "meta.type.parameters",
        "meta.return.type",
        "entity.name.type.interface",
        "meta.type.annotation",
        "meta.function.parameters"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}